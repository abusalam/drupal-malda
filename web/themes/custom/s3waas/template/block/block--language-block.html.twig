{#
/**
 * @file
 * S3WaaS's override for language switcher block
 */
#}
<section id="topBar1" class="wrapper">
  <div class="container">
    <div class="push-right" role="navigation" aria-label="{{ 'Primary'|t }}">
      <div id="accessibility">
        <ul id="accessibilityMenu">
          <li>
            <a href="javascript:void(0);" title="{{ 'Search'|t }}" id="searchBar" class="searchBar">
              <span class="off-css">{{ 'Search'|t }}</span>
              <span class="fa fa-search" aria-hidden="true"></span>
            </a>
          </li>
          {% if not logged_in %}
          <li>
            <a href="/user" title="{{ 'Login'|t }}" id="login-icon" class="loginBar">
              <span class="off-css">{{ 'Login'|t }}</span>
              <i class="fa fa-sign-in-alt" aria-hidden="true"></i>
            </a>
          </li>
          {% endif %}
          <li>
            <a href="javascript:void(0);" title="{{ 'Social Media Links'|t }}" class="show-social-links">
              <span class="fa fa-share-square" aria-hidden="true">
                <span class="icon-facebook"></span>
                <span class="icon-twitter"></span>
                <span class="icon-youtube"></span>
              </span>
              <span class="off-css">{{ 'Social Media Links'|t }}</span>
            </a>
            <div class="socialIcons" role="complementary">
              <ul>
                <li class="ico-social">
                  <ul>
                    <li><a href="#" target="_blank" aria-label="{{ 'Facebook | External site that opens in a new window'|t }}"><img src="/{{ directory }}/images/icons/ico-facebook.png" title="Facebook | External site that opens in a new window" alt="Facebook | External site that opens in a new window"></a></li>
                    <li><a href="#" target="_blank" aria-label="{{ 'Twitter | External site that opens in a new window'|t }}"><img src="/{{ directory }}/images/icons/ico-twitter.png" title="Twitter | External site that opens in a new window" alt="Twitter | External site that opens in a new window"></a></li>
                    <li><a href="#" target="_blank" aria-label="{{ 'Youtube | External site that opens in a new window'|t }}"><img src="/{{ directory }}/images/icons/ico-youtube.png" title="Youtube | External site that opens in a new window" alt="Youtube | External site that opens in a new window"></a></li>
                  </ul>
                </li>
              </ul>
            </div>
          </li>
          <li>
            <a href="/sitemap" title="{{ 'Sitemap'|t }}">
              <span class="off-css">{{ 'Site Map'|t }}</span>
              <span class="fa fa-sitemap" aria-hidden="true"></span>
            </a>
          </li>
          <li>
            <a href="javascript:void(0);" class="font-size" id="increase" aria-label="{{ 'Font Size Increase'|t }}" title="{{ 'Font Size Increase'|t }}">
              <span aria-hidden="true">A+</span>
            </a>
          </li>
          <li>
            <a href="javascript:void(0);" class="font-size" id="normal" aria-label="{{ 'Normal Font'|t }}" title="{{ 'Normal Font'|t }}">
              <span aria-hidden="true">A&nbsp;</span>
            </a>
          </li>
          <li>
            <a href="javascript:void(0);" class="font-size" id="decrease" aria-label="{{ 'Font Size Decrease'|t }}" title="{{ 'Font Size Decrease'|t }}">
              <span aria-hidden="true">A-</span>
            </a>
          </li>
          <li class="highContrast dark tog-con">
            <a href="javascript:void(0);" class="contrast" id="dark" aria-label="{{ 'High Contrast'|t }}" title="{{ 'High Contrast'|t }}">
              <span aria-hidden="true">A</span>
            </a>
          </li>
          <li class="highContrast light">
            <a href="javascript:void(0);" class="contrast" id="light" aria-hidden="true" aria-label="{{ 'Normal Contrast'|t }}" title="{{ 'Normal Contrast'|t }}">
              <span aria-hidden="true">A</span>
            </a>
          </li>
          <li>
            <a class="lang-switcher change-language" aria-label="{{ switch_lang }}" href="{{ switch_path }}">
              {{ switch_lang }}
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div class="push-left">
      <div class="govBranding">
        <ul>
          <li>
            <a href="http://wb.gov.in" lang="grt">{{ 'Government of West Bengal'|t }}</a>
          </li>
          <li>
            <a href="{{ path('<front>') }}">{{ 'Malda District'|t }}</a>
          </li>
        </ul>
      </div>
    </div>
  </div>
</section>
